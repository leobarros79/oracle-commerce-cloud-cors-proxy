<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>oracle-commerce-cloud-cors-proxy</display-name>
  <context-param>
    <param-name>incomingURLs</param-name>
    <param-value>http://localhost:8080</param-value>
  </context-param>
  <servlet>
    <description>GetNewOrders</description>
    <display-name>GetNewOrders</display-name>
    <servlet-name>GetNewOrders</servlet-name>
    <servlet-class>com.oracle.proxy.GetNewOrders</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetNewOrders</servlet-name>
    <url-pattern>/GetNewOrders</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>